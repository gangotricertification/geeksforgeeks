#include<bits/stdc++.h>
using namespace std;
int main()
{
	//code
	int t;
	cin>>t;
	while(t--)
	{
	    int n,k,i;
	    cin>>n>>k;
	    vector<int>v(n);
	    for(i=0;i<n;i++)
	    cin>>v[i];
	    sort(v.begin(),v.end());
	    for(i=n-1;i>=n-k;i--)
	    cout<<v[i]<<" ";
	    cout<<endl;
	}
	return 0;
}
